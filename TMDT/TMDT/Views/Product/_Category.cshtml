@model IEnumerable<ProjectSEM3.EF.Producer>
<ul id="collapsed-menu">
    <li class="level0 nav-1 first parent active">
        <ul class="nav nav-list tree" style="display: block;">
            @foreach (var item in Model)
            {
                <li class="level1 nav-1-1 active first parent">
                    <label class="tree-toggler nav-header closed"> <a href="/dong-ho/@item.MetaTitle-@item.ID">@item.Name</a><span class="collapse_button">+</span></label>
                </li>
            }


        </ul>
    </li>
</ul>